{
  "expo": {
    "name": "Mimic",
    "slug": "mimic-native",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000a13"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nl.thijsmolendijk.mimic",
      "buildNumber": "7"
    },
    "android": {
      "package": "nl.thijsmolendijk.mimic",
      "versionCode": 7,
      "useNextNotificationsApi": true
    }
  }
}
